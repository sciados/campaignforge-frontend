        "security": [{ "HTTPBearer": [] }],
        "parameters": [
          {
            "name": "submission_id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "title": "Submission Id" }
          },
          {
            "name": "approve",
            "in": "query",
            "required": false,
            "schema": { "type": "boolean", "default": true, "title": "Approve" }
